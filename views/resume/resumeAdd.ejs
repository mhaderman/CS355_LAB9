<%- include("../includes/header.ejs", {title:"Resume - Add"}); %>

<form action="/resume/insert" method="get">

    <input type="hidden" id="account_id" name="account_id" value="<%= account.account_id %>">
    <br/>
    <br/>
    Skills
    <br/>
    <select id="skill_id" name="skill_id">
        <% for(var i=0; skill.length > i; i++ ) { %>
        <option value="<%= skill[i].skill_id %>"><%= skill[i].skill_name + ', ' + skill[i].description %></option>
        <% } %>
        <br/>
    </select>
    <input type="submit" />

</form>

<%- include("../includes/footer.ejs"); %>