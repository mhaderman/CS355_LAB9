<%- include("../includes/header.ejs", {title:"Add New Company"}); %>
    <h1>Add New Company</h1>
    <form action="/company/insert" method="get">
        Company Name: <input id="company_name" name="company_name" type="text" />
        <br />
        <br />
        Address
        <br />
        <select id="address_id" name="address_id">
            <% for(var i=0; address.length > i; i++ ) { %>
                <option value="<%= address[i].address_id %>"><%= address[i].street + ', ' + address[i].zip_code %></option>
            <% } %>
        </select>

        <br />
        <input type="submit" />

    </form>
<%- include("../includes/footer.ejs"); %>